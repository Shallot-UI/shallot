{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "test:watch": {
      "cache": false
    },
    "test:coverage": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "check-types": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
